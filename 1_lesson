    <script>
              
        let Tc = prompt('Введите температуру по Цельсию');
        let Tf = (9 / 5) * Tc + 32;
        
        alert(`Температура по Цельсию: ${Tc}\n` +
              `Температура по Фаренгейту ${Tf}`);    
              
        //   Объявить две переменные: admin и name.
        //   Записать в name строку "Василий";
        //   Скопировать значение из name в admin. 
        //   Вывести admin (должно вывести «Василий»).
        let admin = '';
        let name = '';
        name = 'Василий';
        admin = name;
        alert(admin);  

